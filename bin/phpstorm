#!/bin/sh
#Generated by JetBrains Toolbox 1.20.7940 at 2021-04-09T11:25:50.319897

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/rihardsgrigorjevs/Library/Application Support/JetBrains/Toolbox/apps/PhpStorm/ch-0/211.6693.120/PhpStorm.app/Contents/MacOS/phpstorm" $wait --args "${ideargs[@]}"