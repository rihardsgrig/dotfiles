#!/bin/sh
#Generated by JetBrains Toolbox 1.19.7784 at 2021-01-22T10:08:59.225342

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/rihardsgrigorjevs/Library/Application Support/JetBrains/Toolbox/apps/PhpStorm/ch-0/201.8538.41/PhpStorm.app/Contents/MacOS/phpstorm" $wait --args "${ideargs[@]}"